{
  "name": "@dimano/ts-devtools-plugin-prefetch-heatmap",
  "version": "0.1.9",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": [
    "dist/overlay-style.css"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && shx mkdir -p dist && shx cp src/overlay-style.css dist/overlay-style.css",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18.2.0 <20",
    "react-dom": ">=18.2.0 <20"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "shx": "^0.3.4",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimitrianoudi/tanstack-prefetch-heatmap"
  },
  "homepage": "https://github.com/dimitrianoudi/tanstack-prefetch-heatmap#readme",
  "bugs": {
    "url": "https://github.com/dimitrianoudi/tanstack-prefetch-heatmap/issues"
  },
  "keywords": [
    "tanstack",
    "devtools",
    "react",
    "router",
    "prefetch",
    "heatmap"
  ]
}
